<%= form_with(model: @user) do |f| %>
  <%= f.label :name, 'User Name' %>
  <%= f.text_field :name %>
  <%= f.label :email, 'Mail Address' %>
  <%= f.email_field :email, placeholder: 'you@example.com' %>
  <%= f.label :password, 'Login Password' %>
  <%= f.password_field :password %>
  <%= f.submit 'Sign Up!!' %>
<% end %>